{
  "build": {
    "content": [
      {
        "src":"user-guide/en",
        "dest":"en/learn",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ]
      }
    ],
    "resource": [
      {
        "src":"user-guide/media",
        "dest":"media/learn",
        "files": [ "**/*.{png,svg,jpg,gif,bmp,zip}" ]
      },
      {
        "src":"user-guide/en",
        "dest":"media/learn/en",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip}"]
      },
      {
        "src":"common/icons/",
        "dest":"media/common",
        "files": [ "**/*.png" ]
      }
    ],
    "xref": [
      "No"
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./build-docs/templates/superoffice"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SuperOffice Help Files",
      "_appFooter": "Â© SuperOffice. All rights reserved.",
      "_appLogoPath": "assets/images/logo.svg",
      "_appFaviconPath": "assets/images/favicon.ico",
      "_enableSearch": false
    }
  }
}
