{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}",
          "*.{md,yml}"
        ],
        "exclude": [
          "docs/api/**/*.*",
          "filters/**/*.*",
          "build/**/*.*",
          "samples/**/*.*",
          "**/includes/**",
          "**/util/**",
          "***/contributing.md",
          "**/tmp/**",
          "**/_site/**",
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**/*.*",
          "media/**/*.*",
          "build/**/*.*",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.gif",
          "**/*.bmp",
          "**/*.xlsx",
          "**/*.zip",
          "**/*.dll",
          "**/*.docx",
          "**/*.pdf",
          "**/*.pptx",
          "**/*.app"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "./templates/default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}