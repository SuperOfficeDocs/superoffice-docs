{
  "metadata": [],
  "build": {
    "content": [
      {
        "exclude": ["**/includes/**"]
      },
      {
        "src":"docs/assets/downloads",
        "dest":"downloads",
        "files": ["**/index.md"]
      },
      {
        "src":"docs",
        "dest":"",
        "files": ["index.md", "toc.yml"]
      },
      {
        "src":"docs/en",
        "dest":"en/",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude":[ "toc.yml","index.md", "api/" ]
      },
      {
        "src":"docs/en/api",
        "dest":"en/api",
        "files": [ "toc.yml","index.md", "index.yml" ]
      },
      {
        "src":"docs/en/api",
        "dest":"en/api/netserver",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude":[ "toc.yml","index.md", "index.yml", "security/", "overview/", "tutorials/", "reference/", "tooltip/", "web-extensions/", "nuget/" ]
      },
      {
        "src":"docs/en/api/overview",
        "dest":"en/api/overview",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ]
      },
      {
        "src":"docs/en/api/security",
        "dest":"en/api/security",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude":[ "toc.yml" ]
      },
      {
        "src":"docs/en/api/tutorials",
        "dest":"en/api/tutorials",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude":[ "toc.yml" ]
      },
      {
        "src":"docs/no",
        "dest":"no/",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude":[ "toc.yml","index.md"]
      },
      {
        "src":"release-notes",
        "dest":"release-notes",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude": ["**/includes/**"]
      },
      {
        "src":"integrations",
        "dest":"integrations",
        "files": [ "**/**.{md,yml}", "*.{md,yml}" ],
        "exclude": ["**/includes/**"]
      }
    ],
    "resource": [
      {
        "src":"release-notes",
        "dest":"release-notes",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip}"]
      },
      {
        "src":"integrations",
        "dest":"integrations",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip}"]
      },
      {
        "src":"docs/assets/downloads",
        "dest":"downloads",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip,xlsx,pptx,app,docx,pdf,dll,txt}"]
      },
      {
        "files": [ "assets/**" ]
      },
      {
        "src":"docs/media",
        "dest":"media",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip}"]
      },
      {
        "src":"docs/en",
        "dest":"en/media",
        "files": ["**/*.{png,svg,jpg,gif,bmp,zip}"]
      },
      {
        "src":"common/icons/",
        "dest":"media/common",
        "files": [ "**/*.png" ]
      }
    ],
    "overwrite": [],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "fileMetadata":
      {
        "_noindex":
        {
          "docs/en/api/reference/**": true,
          "docs/en/api/archive-providers/reference/**": true,
          "docs/en/automation/crmscript/reference/**": true
        }
      },
    "template": [
      "default",
      "./build-docs/templates/superoffice"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "SuperOffice Docs",
      "_appFooter": "Â© SuperOffice. All rights reserved.",
      "_appLogoPath": "assets/images/logo.svg",
      "_appFaviconPath": "assets/images/favicon.ico",
      "breadcrumb_path": "/build-docs/breadcrumb/toc.yml",
      "_enableSearch": false,
      "_enableNewTab": true,
      "_disableNavbar": false,
      "_disableBreadcrumb": false,
      "_disableToc": false,
      "_disableAffix": false,
      "_disableContribution": false
    },
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "sitemap": {
      "baseUrl": "https://docs.superoffice.com/"
    }
  }
}