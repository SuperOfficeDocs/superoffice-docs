edge [fontname = "Helvetica" fontsize = "9"]

ticket_log_change[fillcolor = "cadetblue1"]
ticket[fillcolor = "azure1"]
ej_message[fillcolor = "azure1"]
ticket_log_action[fillcolor = "azure1"]
