                            <span class="activity-time" data-bind="html: $data.TimePublished"></span>
                            <span class="action-text">

                                <!-- ko if: $data.Header().length > 0 -->

                                <!-- ko ifnot: $data.ActivityType() === 'actionForumTopic' || $data.ActivityType() === 'actionForumReply' || $data.ActivityType() === 'cmsPageComment' || $data.ActivityType() === 'actionBlogComment' || $data.ActivityType() === 'actionVideoComment' -->
                                <span data-bind="html: $root.GetActivity($data.ActivityType, $data.Header, $data.Url)"></span>
                                <!-- /ko -->
                                
                                <!-- ko if: $data.ActivityType() === 'cmsPageComment' || $data.ActivityType() === 'actionBlogComment' || $data.ActivityType() === 'actionVideoComment' -->
                                <span data-bind="html: $root.GetCommentActivity($data.ActivityType, $data.Header, $data.EntityParent, $data.Url)"></span>
                                <!-- /ko -->

                                <!-- ko if: $data.ActivityType() == 'actionForumTopic' -->
                                <span data-bind="html: $root.GetForumTopic($data.Header, $data.Url, $data.EntityParent, $data.EntityParentUrl)"></span>
                                <!-- /ko -->

                                <!-- ko if: $data.ActivityType() == 'actionForumReply' -->
                                <span data-bind="html: $root.GetForumReply($data.Header, $data.ActivityRepliedTo, $data.Url, $data.EntityParent, $data.EntityParentUrl)"></span>
