    function capitalizeFirstLetter(string) {
        return string.charAt(0).toUpperCase() + string.slice(1);
    }

    window.getShortMonthName = function (month) {
        switch (month) {
        case 1:
            return capitalizeFirstLetter("Jan");
        case 2:
            return capitalizeFirstLetter("Feb");
        case 3:
            return capitalizeFirstLetter("Mar");
        case 4:
            return capitalizeFirstLetter("Apr");
        case 5:
            return capitalizeFirstLetter("May");
        case 6:
            return capitalizeFirstLetter("Jun");
        case 7:
            return capitalizeFirstLetter("Jul");
        case 8:
            return capitalizeFirstLetter("Aug");
        case 9:
            return capitalizeFirstLetter("Sep");
        case 10:
            return capitalizeFirstLetter("Oct");
        case 11:
            return capitalizeFirstLetter("Nov");
        case 12:
            return capitalizeFirstLetter("Dec");
        default:
            return "";
        }
    }

    window.getLongMonthName = function (month) {
        switch (month) {
        case 1:
            return capitalizeFirstLetter("January");
        case 2:
            return capitalizeFirstLetter("February");
        case 3:
            return capitalizeFirstLetter("March");
        case 4:
            return capitalizeFirstLetter("April");
        case 5:
            return capitalizeFirstLetter("May");
        case 6:
            return capitalizeFirstLetter("June");
        case 7:
            return capitalizeFirstLetter("July");
        case 8:
            return capitalizeFirstLetter("August");
        case 9:
            return capitalizeFirstLetter("September");
        case 10:
            return capitalizeFirstLetter("October");
        case 11:
            return capitalizeFirstLetter("November");
        case 12:
            return capitalizeFirstLetter("December");
        default:
            return "";
        }
    }
