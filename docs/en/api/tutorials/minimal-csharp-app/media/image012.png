    });

    $('#closeEventDialog').on("click", function () { 
        $('#newGlobalEventContainer').css("display", "none");
        $.cookie("showDialog", "0");
    });

    var prevDate = new Date(1900, 1, 1, 0, 0, 0, 0);
    var eventSubsidiary = "0";
    var eventLanguage = "all";
    var upcomingEventsViewModel = new UpcomingEventsViewModel();
    var clubEventUrl = '/en/events/';
    var serverTimeZoneOffsetHours = 1;

    ko.applyBindings(upcomingEventsViewModel, document.getElementById("WhatsHappeningWrapper"));

    function UpcomingEventsViewModel() {
        var self = this;
        self.results = ko.observable([]);
        self.loading = ko.observable(false);
        self.filterBy = 'Language';

        self.separatorIndex = ko.computed(function () {
            var breakIndex = 0;
            var previousDate = new Date(1900, 1, 1, 0, 0, 0, 0);
            var date;
            for (var i = 0; i < self.results().length; i++) {
                date = new Date(parseInt(self.results()[i].Start.substr(6)));
                if (i > 0) {
                    if (date.format("dd/MM/yyyy") === previousDate.format("dd/MM/yyyy")) {
                        breakIndex = i;
                    } else {
                        break;
                    }
                }
                previousDate = date;
            }
            if (breakIndex === self.results().length - 1) {
                breakIndex = self.results().length;
            }
            return breakIndex;
        });

        self.getUpcomingEvents = function (subsidiaryId, language) {
            $('.toolWrapper').slideUp();
            $('#upcomingEventList').slideUp();
            self.loading(true);
            $.get('/templates/SuperNet/Handlers/EventHandler.ashx', { SubsidiaryId: subsidiaryId, AreaId: '', Language: language, r: new Date().getMilliseconds() }, function (data) {
                self.results(data);
            }).always(function () {
                self.loading(false);
                $('.toolWrapper').slideDown();
                $('#upcomingEventList').slideDown();
            });
        };

        self.GetUrl = function (urlSegment) {
            return clubEventUrl + urlSegment;
        };

        self.GetBodyText = function (bodyText, chars) {
            var text = bodyText;
            var limit = chars;

            if (text !== "") {
                if (text.length > limit) {
                    text = text.substring(0, limit) + "...";
                }
            }
            return text;
        };

        self.showPopup = function(id) {
            $("#event-popup-"+id).slideDown();
        };

        self.hidePopup = function(id) {
            var isHovered = $("#event-popup-"+id).is(":hover"); 
            if (!isHovered) {
                $("#event-popup-"+id).slideUp();
            }
        };

        self.DisplayDateFormat = {
            DateOnly: 1,
            MonthOnly: 2
        };

        self.addZero = function(i) {
            return i < 10 ? "0" + i : i;
        }

        self.getFormattedDate = function (date, dateFormat) {
            date.setHours(date.getHours() + serverTimeZoneOffsetHours);
            switch (dateFormat) {
                case self.DisplayDateFormat.DateOnly:
                    return self.addZero(date.getUTCDate());
                case self.DisplayDateFormat.MonthOnly:
                    return window.getLongMonthName(date.getUTCMonth() + 1);
                default:
                    return date;
            }
        };

        self.dateToShow = function (datePublished, dateFormat, hideDate) {
            var date = datePublished;
            if (typeof date.getMonth !== 'function') {
                date = new Date(parseInt(datePublished.substr(6)));
            }
            if (hideDate) {
                if (prevDate.format("dd/MM/yyyy") === date.format("dd/MM/yyyy")) {
                    return '';
                }
            }
            prevDate = date;
            return self.getFormattedDate(date, dateFormat);
        };

        self.sortSubsidiaries = function(a, b) {
            return $.trim($(a).find("a.sortItem").text()).localeCompare($.trim($(b).find("a.sortItem").text()));
        };

        self.initCountryDropdown = function () {
            $('#sel-option').hide();

            $.get('/templates/SuperNet/Handlers/GetSubsidiaries.ashx', function (subsidiaries) {
                var firstItem = true;
                $.each(subsidiaries, function(key, value) {
                    $('#sel-option')
                        .append($("<li></li>")
                            .append($("<a class='subItem" + (firstItem ? "" : " sortItem") + "'></a>")
                                .attr("href", key)
                                .text(value)));
                    firstItem = false;
                });

                var sortedSubsidiaries = $("ul#sel-option li").has("a.sortItem").sort(self.sortSubsidiaries);
                $('#sel-option').append(sortedSubsidiaries);

                $('#country-options').text($('#sel-option .subItem').first().text());

                $('#sel-option a').on("click", function (e) { 
                    if (eventSubsidiary !== $(this).attr('href')) {
                        eventSubsidiary = $(this).attr('href');
                        self.getUpcomingEvents(eventSubsidiary, "all");
                        $('#country-options').text($(this).text());
                    }
                    $('#sel-option').slideUp();
                    e.preventDefault();
                });
            });

            $("#country-options").on("click", function () { 
                $("#sel-option").slideToggle();
            });

            $('.options').mouseleave(function () {
                $("#sel-option").slideUp();
            });
        };

        self.initLanguageDropdown = function () {
            $('#sel-option').hide();

            var languages = {"all":"All Languages","da":"Danish","nl":"Dutch","en":"English","de":"German","no":"Norwegian","se":"Swedish"};
            $.each(languages, function (key, value) {
                $('#sel-option')
                    .append($("<li></li>")
                        .append($("<a class='subItem'></a>")
                            .attr("href", key)
                            .text(value)));
            });

            $('#country-options').text($('#sel-option .subItem').first().text());

            $('#sel-option a').on("click", function (e) { 
                if (eventLanguage !== $(this).attr('href')) {
                    eventLanguage = $(this).attr('href');
                    self.getUpcomingEvents("0", eventLanguage);
                    $('#country-options').text($(this).text());
                }
                $('#sel-option').slideUp();
                e.preventDefault();
            });

            $("#country-options").on("click", function () { 
                $("#sel-option").slideToggle();
            });

            $('.options').mouseleave(function () {
                $("#sel-option").slideUp();
            });
        };

        if (self.filterBy === 'Language') {
            self.initLanguageDropdown();
            self.getUpcomingEvents("0", eventLanguage);
        } else {
            self.initCountryDropdown();
            self.getUpcomingEvents(eventSubsidiary, "all");
        }

        ko.bindingHandlers.hoverIntent = {
            update: function(elem, value) {
                var options = ko.utils.unwrapObservable(value());
                if (typeof options === 'object') {
                    for (var option in options)
                        options[option] = ko.utils.unwrapObservable(options[option]);
                }
                $(elem).hoverIntent(options);
            }
        }
    }
</script>

