        //        });

        //    });
        //}

        // Hide mega menu on click
        $oe_menu_items.on("click", function (e) { 
            $oe_menu.trigger('mouseleave');
        });

        function HideMenu(elm) {
            if (elm && elm.length > 0) {
                var $this = elm;
            } else {
                var $this = $(this);
            }
            $this.removeClass('selected2').children('div').css('z-index', '1');
        }

        function ShowMenu(elm) {
            if (elm && elm.length > 0) {
                var $this = elm;
            } else {
                var $this = $(this);
            }

            var $oe_menu = $('#oe_menu');
            var $oe_menu_items = $oe_menu.children('li');
            var $oe_overlay = $('#oe_overlay');
            if (!$oe_menu.hasClass("hovered")) {
                $oe_menu.css("height", "34px");
                $oe_overlay.stop(true, true).fadeTo(200, 0.6);
                $oe_menu.addClass('hovered');
            }

            $oe_menu_items.removeClass('slided selected2');
            $this.addClass('slided selected2');
            $this.children('div').css('z-index', '9999').stop(true, true).slideDown(200, function () {
                $oe_menu_items.not('.slided').children('div').hide();
                $this.removeClass('slided');
            });
        }
    });

