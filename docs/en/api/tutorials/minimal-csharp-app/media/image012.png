    </div>
    <script type="text/javascript">

        var activityViewModel = new ActivityViewModel();
        ko.applyBindings(activityViewModel, document.getElementById("activityFeedModelWrapper"));
        var communityStreamHub = $.connection.communityStreamHub;
        $.connection.hub.start();
        var communityStreamUpdateTimer;
        var currentClubId = 0;
        
        var prevDate = "";
        function ActivityViewModel() {
            var self = this;
            self.feedResults = ko.observableArray([]);
            self.loading = ko.observable(false);
            self.page = ko.observable(2);
            self.total = 0;
            self.pageSize = '8';

            // Auto load new activities - to do refactor
            self.getNewItem = function() {
                $.get('/templates/SuperNet/Handlers/ActivityFeedHandler.ashx', { clubId: currentClubId, showUserImg: "true", page: 1, pagesize: 1, r: new Date().getMilliseconds()}, function (data) {
                    var newItemArray = ko.observableArray([]);
                    addToObservableArray(newItemArray, data.items);
                    self.feedResults.unshift(newItemArray()[0]);
                    self.feedResults.pop();
                    self.loading(false);
                    self.showFirstDate();
                }).always(function () {
                    $(".search-list div:first").hide().fadeIn('fast');
                    $(".search-list div.SOcommentBlog:not(.doNotShowJet):last").hide();
                });
            };

            // Get activities 
            self.getActivites = function (page) {
                self.loading(true);
                $.get('/templates/SuperNet/Handlers/ActivityFeedHandler.ashx', { clubId: "0", showUserImg: "true", page: page, pagesize: self.pageSize, r: new Date().getMilliseconds() }, function(data) {
                    if (data != null) {
                        addToObservableArray(self.feedResults, data.items);
                        self.total = data.total;
                    }
                }).always(function() {
                    self.loading(false);
                    self.showFirstDate();
                });
            };
            self.getActivites(1);

            self.ShowDateHeading = function(datePublished) {
                if (datePublished === prevDate) {
                    return false;
                } else {
                    prevDate = datePublished;
                    return true;
                }
            };

            // Due to calling the data-bindins twice, the date becomes invisible if the stream has only one date
            // To fix this issues, showFirstDate implicitly 
            self.showFirstDate = function() {
                if (!self.loading()) {
                    $('.newsFeedDate').first().show();
                }
            };

            self.DateToShow = function(datePublished) {
                var dateParts = datePublished.split("-");
                var date = new Date(dateParts[2], dateParts[1] - 1, dateParts[0]);
                var now = new Date();
                if (datePublished === now.format("dd-MM-yyyy")) {
                    return "Today";
                } else {
                    return date.format("d MMM yyyy");
                }
            };

            self.GetActivityText = function(activityType) {
                var text = null;

                switch (activityType()) {

                case 'cmsPageComment':
                    text = 'Commented - {0} on article {1}';   
                        break;
                    case 'actionBlogComment':
                        text = 'Commented - {0} on blog entry {1}';  
                        break;
                    case 'actionBlogEntry':
                        text = 'Blogged {0}';
                        break;
                    case 'actionVideoComment':
                        text = 'Commented - {0} on video {1}';    
                        break;
                    case 'actionVideoUpload':
                        text = 'Uploaded a video {0}';
                        break;
                    case 'actionPublishedArticle':
                        text = 'Published {0}';
                        break;
                    case 'actionPublishedResources':
                        text = 'Made {0}';
                        break;
                    case 'articleUpdate':
                    case 'resourceUpdate':
                        text = 'Updated {0}';
                        break;
                    case 'actionNewEvent': 
                        text = 'Created a new event {0}';
                        break;
                    default:
                        break;
                }

                return text;
            };

            self.GetActivity = function(activityType, header, url) {
                //We can live with the language error, it is better to have the link in the stream.
                var activityText = this.GetActivityText(activityType);
                var headerLink = '<a title="' + header() + '" href="' + url() + '"><span>' + header() + '</span></a>';
                return String.format(activityText, headerLink);
            };

            self.GetCommentActivity = function(activityType, comment, parent, parentUrl) {
                var activityText = this.GetActivityText(activityType);
                var commentText = '<span>' + comment() + '</span><br/>';
                var parentLink = '<a title="' + parent() + '" href="' + parentUrl() + '"><span>' + parent() + '</span></a>';
                return String.format(activityText, commentText, parentLink);
            };

            self.GetForumTopic = function(topic, topicUrl, parent, parentUrl) {
                var activityText = 'Posted - {0} in {1} ';
                var topicLink = '<a title="' + topic() + '" href="' + topicUrl() + '"><span>' + topic() + '</span></a><br/>';
                var parentLink = '<a title="' + parent() + '" href="' + parentUrl() + '"><span>' + parent() + '</span></a>';
                return String.format(activityText, topicLink, parentLink);
            };

            self.GetForumReply = function(reply, topic, topicUrl, parent, parentUrl) {
                var activityText = 'Replied - {0} to {1} in {2} ';
                var replyText = '<span>' + reply() + '</span><br/>';
                var topicLink = '<a title="' + topic() + '" href="' + topicUrl() + '"><span>' + topic() + '</span></a>';
                var parentLink = '<a title="' + parent() + '" href="' + parentUrl() + '"><span>' + parent() + '</span></a>';
                return String.format(activityText, replyText, topicLink, parentLink);
            };

            self.loadMoreActivities = function () {
                self.getActivites(self.page());
                self.page(self.page() + 1);
                $('#loadMore').trigger( "blur" );
            };

            self.getBadgeStyle = function(role) {
                if (role!== '') {
                    return 'badge-tag ' + role;
                }
                return 'badge-tag';
            };

            var addToObservableArray = function (currentArray, data) {
                var peopleItems = ko.observableArray();
                ko.mapping.fromJS(data, {}, peopleItems);
                self.loading(true);
                currentArray.valueWillMutate();
                ko.utils.arrayPushAll(currentArray, peopleItems());
                currentArray.valueHasMutated();

            };
        }

        // update activity feed using SignalR
        communityStreamHub.client.sendActivityFeedUpdate = function (clubId) {
            if (clubId === currentClubId) {
                // updates are only allowed at a maximum refresh rate of 1.5 second.
                clearTimeout(communityStreamUpdateTimer);
                communityStreamUpdateTimer = setTimeout(function () {
                    activityViewModel.getNewItem();
                }, 1500);
            }
        };

        // String format for javascript
        String.format = function() {
            // The string containing the format items (e.g. "{0}")
            // will and always has to be the first argument.
            var theString = arguments[0];

            // start with the second argument (i = 1)
            for (var i = 1; i < arguments.length; i++) {
                // "gm" = RegEx options for Global search (more than one instance)
                // and for Multiline search
                var regEx = new RegExp("\\{" + (i - 1) + "\\}", "gm");
                theString = theString.replace(regEx, arguments[i]);
            }

            return theString;
        };

    </script>

			</div>

