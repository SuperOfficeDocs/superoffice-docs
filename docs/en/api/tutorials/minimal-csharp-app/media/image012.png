<script type="text/javascript">
    $(document).ready(function () {
        $('#searchfield').focus();
        var showWelcomeMessage = '0';
        var isMasterResponsive = 'True';
        var isGlobalLoginVisible = $('.globalLoginBox').is(':visible');

        if ($('.editMessage').text().trim() !== '') {
            $('body').css('margin-top', $('.IT-message-wrapper').height() + 'px');
            $('.IT-message-wrapper').show();
            $(window).resize(function () {
                $('body').css('margin-top', $('.IT-message-wrapper').height() + 'px');
            });
        } else {
            $('.IT-message-wrapper').hide();
            $('body').css('margin-top', '0');
        }

        //Do not show splash screen when embedded in an iframe
        if (SuperOffice.Community.Util.inIframe()) {
            return false;
        }

        if (showWelcomeMessage === '1' && getCookie('welcome') == null && !isGlobalLoginVisible && isMasterResponsive === 'True') {
            $('#splashScreen').css('display', 'block');
            setCookie('welcome', 'displayed', (10 * 365));
        }

        // Set focus to main content when skip to main content
        $(".skip").on("click", function (event) { 
            var skipTo = "#" + this.href.split('#')[1];
            $(skipTo).attr('tabindex', -1).on('blur focusout', function () {
                $(this).prop( "tabindex", false );
            }).focus();
        });

        
        //Go to manage subscription
        $('#manageSubscription').on("click", function () {
            var newWindow = window.open('', '_blank');
            $.get('/Templates/SuperNet/Handlers/SubscriptionHandler.ashx',
                function(url) {
                    newWindow.location = url;
                    newWindow.focus();
                });
        });

    });

    $("li.language-selection").on("click", function () {
        $("div.language-dropdown-wrapper").toggle();
        $('#headerLanguageSelector').attr('aria-expanded', $('#headerLanguageSelector').attr('aria-expanded') === 'false' ? 'true' : 'false');
    });

    $("li.language-selection").mouseleave(function () {
        $("div.language-dropdown-wrapper").slideUp();
        $('#headerLanguageSelector').attr('aria-expanded', $('#headerLanguageSelector').attr('aria-expanded') === 'false' ? 'true' : 'false');
    });

    $("li.profile-selection").on("click", function () {
        if (!$('.mobile-menu-close').is(':visible')) {
            $("div.profile-dropdown-wrapper").toggle();
        }
    });

    $("li.profile-selection").mouseleave(function () {
        if (!$('.mobile-menu-close').is(':visible')) {
            $("div.profile-dropdown-wrapper").slideUp();
        }
    });

    $("span.search-menu-icon").on("click", function () {
        $("div.search-control-wrapper").toggle();
        $(".search-input").val('');
        $(".search-input").focus();
    });
    $("span.search-menu-text").on("click", function () {
        $("div.search-control-wrapper").toggle();
        $(".search-input").val('');
        $(".search-input").focus();
    });
    $(".mobile-menu-trigger").on("click", function () {
        $(".mobile-menu-close").toggle();
        $(".mobile-menu-trigger").toggle();
        $(".nav-secondary").toggle();
        $(".oe_wrapper").toggle();
        $("div.profile-dropdown-wrapper").show();
    });
    $(".mobile-menu-close").on("click", function () {
        $(".mobile-menu-trigger").toggle();
        $(".mobile-menu-close").toggle();
        $(".nav-secondary").toggle();
        $(".oe_wrapper").toggle();
        $("div.profile-dropdown-wrapper").hide();
