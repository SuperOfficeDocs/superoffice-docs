        swap_val = [];
        $(".inputswap").each(function(i) {
            swap_val[i] = $(this).val();
            $(this).focusin(function() {
                if ($(this).val() == swap_val[i]) {
                    $(this).val("");
                }
            }).focusout(function() {
                if ($.trim($(this).val()) == "") {
                    $(this).val(swap_val[i]);
                }
            });
        });
        $(".close-button").on("click", function () { 
            $(".search-control-wrapper").css('display','none');
        });
        
    });
