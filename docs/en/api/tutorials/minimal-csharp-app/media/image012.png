            });

            $("body").on("click", function () { 
                HideMenu(defThis);
                var $this = defThis;
                $this.css("height", "auto");
                $('#oe_menu').removeClass('hovered');

                $oe_overlay.stop(true, true).fadeTo(200, 0);
                $oe_overlay.hide(200);
